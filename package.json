    "docs:changelog": "bash scripts/generate-changelog.sh",
    "deploy:prepare": "bash scripts/prepare-deployment.sh",
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build:client": "vite build",
    "build:server": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist-server",
    "build": "npm run build:client && npm run build:server",
    "start": "NODE_ENV=production node dist/server/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:local": "node scripts/test-simple.js",
    "test:local:setup": "cp local-test-config.env .env && echo '로컬 테스트 환경 설정 완료'",
    "check:env": "node scripts/check-env-with-load.js",
    "setup:prod": "cp production.env .env && echo '프로덕션 환경변수 설정 완료'",
    "verify:startup": "node scripts/verify-app-startup.js",
    "setup:workflows": "node scripts/setup-data-processing-workflows.js",
    "setup:workflows:test": "node scripts/setup-data-processing-workflows.js --test",
    "setup:workflows:rollback": "node scripts/setup-data-processing-workflows.js --rollback",
    "validate:workflow": "tsx scripts/validate-workflow-consistency.mjs",
    "init:workflow-resources": "tsx scripts/init-workflow-resources.mjs",
    "validate:schema": "tsx scripts/validate-schema-loading.ts",
    "prebuild": "npm run validate:schema"
